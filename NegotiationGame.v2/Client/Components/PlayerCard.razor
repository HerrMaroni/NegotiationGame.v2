<MudPaper Class="mud-theme-info py-1 mr-auto" style="max-width: 200px; width: fit-content; height: fit-content;">
    <MudStack Justify="Justify.Center" AlignItems="AlignItems.Center" Spacing="0">
        <MudText Typo="Typo.h4" Align="Align.Center">@Name</MudText>
        <div style="max-width:300px;">
            @switch (Player)
            {
                case "player1":
                    <svg viewBox="0 0 180 180" width="100%" height="100%" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <defs>
                            <g id="widget307">
                                <image preserveAspectRatio="xMidYMid" style="overflow:visible" width="100%" height="100%"
                                       xlink:href="data:image/png;base64,"
                                       x="73px" y="780px"/>
                            </g>
                        </defs>
                        <g transform="matrix(1 0 0 1 -73 -780 )">
                            <use xlink:href="#widget307"/>
                        </g>
                    </svg>
                    break;
                case "player2":
                    <svg viewBox="0 0 180 180" width="100%" height="100%" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <defs>
                            <g id="widget305">
                                <image preserveAspectRatio="xMidYMid" style="overflow:visible" width="100%" height="100%" xlink:href="data:image/png;base64," x="1190px" y="178px"/>
                            </g>
                        </defs>
                        <g transform="matrix(1 0 0 1 -1190 -178 )">
                            <use xlink:href="#widget305"/>
                        </g>
                    </svg>
                    break;
            }
        </div>
        @if (Player is "player1")
        {
            <MudButton Variant="Variant.Filled" EndIcon="@Icons.Filled.Chat" Color="Color.Primary" Class="mx-auto">Chat</MudButton>
        }
    </MudStack>
</MudPaper>

@code {

    [Parameter]
    public string Player { get; set; } = "player1";
    
    [Parameter]
    public string Name { get; set; } = "";

}